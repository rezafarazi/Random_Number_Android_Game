<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ir.sorapp.game_num_check"
    android:installLocation="preferExternal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Game_num_check">
        <activity
            android:name=".settings_ac"
            android:exported="true" />
        <activity
            android:name=".you_lose_ac"
            android:exported="true" />
        <activity
            android:name=".you_win_ac"
            android:exported="true" />
        <activity
            android:name=".help_ac"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".game_ac"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".menu_ac"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".logo_ac"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>